{
  "name": "@example/starter",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://jonasholbech.github.io/courses-s2022/",
  "scripts": {
    "clean": "rm -rf dist",
    "predev": "npm run clean",
    "dev": "npm-run-all -p live-server start",
    "live-server": "live-server -q --no-browser --open=public/snippets/",
    "start": "astro dev",
    "build": "npm-run-all -p --race live-server astro-build",
    "astro-build": "astro build",
    "preview": "astro preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@astrojs/renderer-svelte": "^0.2.2",
    "astro": "^0.21.12",
    "gh-pages": "^3.2.3",
    "npm-run-all": "^4.1.5"
  }
}
